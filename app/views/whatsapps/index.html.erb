<p id="notice"><%= notice %></p>

<h1>Whatsapps</h1>

<table>
  <thead>
    <tr>
      <th>Numero</th>
      <th>Endereco</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @whatsapps.each do |whatsapp| %>
      <tr>
        <td><%= whatsapp.numero %></td>
        <td><%= whatsapp.endereco %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Whatsapp', new_whatsapp_path %>
