<p id="notice"><%= notice %></p>

<h1>Whatsapps</h1>

<table>
  <thead>
    <tr>
      <th>Inquilino</th>
      <th>Numero</th>
      <th>Endereco</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @whatsapps.each do |whatsapp| %>
      <tr>
        <td><%= whatsapp.inquilino %></td>
        <td><%= whatsapp.numero %></td>
        <td><%= whatsapp.endereco %></td>
        <td><%= link_to 'Show', whatsapp %></td>
        <td><%= link_to 'Edit', edit_whatsapp_path(whatsapp) %></td>
        <td><%= link_to 'Destroy', whatsapp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Whatsapp', new_whatsapp_path %>
